    <test>
        <param name="mode_type" value="only_encoding" />
        <param name="features" value="UserDataExample.csv" />
        <param name="rl_type_choice" value="--ae" />
        <param name="dm" value="40" />
        <output_collection name="encoded_features" type="list">
            <!-- output is non determinisitc -->
            <element name="AE[40]_features" ftype="tabular" >
                <has_n_lines n="20"/>
                <has_n_columns n="40"/>
            </element>
        </output_collection>
    </test>

    <test>
        <param name="mode_type" value="only_encoding" />
        <param name="features" value="UserDataExample.csv" />
        <param name="rl_type_choice" value="--pca" />
        <output_collection name="encoded_features" type="list">
            <element name="PCA_features" file="encodings/PCA_features.tabular" ftype="tabular" />
        </output_collection>
    </test>


    <!-- test multiple parameter sets -->
    <test>
        <conditional name="mode">
            <param name="mode_type" value="only_encoding" />
            <param name="features" value="UserDataExample.csv" />

            <repeat name="parameter_set">
                <conditional name="rl_type">
                    <param name="rl_type_choice" value="--pca" />
                </conditional>
            </repeat>

            <repeat name="parameter_set">
                <conditional name="rl_type">
                    <param name="rl_type_choice" value="--ae" />
                    <param name="dm" value="40" />
                </conditional>
            </repeat>
        
        </conditional>

        <output_collection name="encoded_features" type="list">
            <!-- <element name="AE[40]_features" ftype="tabular" >
                <has_n_lines n="20"/>
                <has_n_columns n="40"/>
            </element> -->
            <element name="PCA_features.tabular" file="encodings/PCA_features.tabular" ftype="tabular" />
         </output_collection>
    </test>  
