<macros>
    <xml name="requirements">
        <requirements>
            <requirement type="package" version="1.41">onto-perl</requirement>
            <requirement type="package">onto-perl</requirement>
            <yield />
        </requirements>
    </xml>
    <xml name="input_ontology">
        <param name="input_ontology" format="obo" type="data" label="Ontology file"/>
    </xml>
    <xml name="term_id">
        <param name="term_id" type="text" size="30" value="" label="Term ID" />
    </xml>
    <xml name="stdio">
        <stdio>
            <exit_code range="1:" />
            <exit_code range=":-1" />
            <regex match="Error:" />
            <regex match="Exception:" />
        </stdio>
    </xml>
    <token name="@VERSION@">1.41</token>
    <xml name="citations">
        <citations>
            <citation type="doi">10.1186/1471-2105-11-S12-S8</citation>
        </citations>
    </xml>
</macros>
